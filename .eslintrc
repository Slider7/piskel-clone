{
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "node": true
  },
  "plugins": [
    "react", 
    "jsx-a11y"
  ],
  "rules": {
    "max-len": [1, 100, 2, {"ignoreComments": true}],
    "react/jsx-uses-react": "error",   
    "react/jsx-uses-vars": "error" 
   },
  "extends": ["airbnb-base"]
}
